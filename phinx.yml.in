paths:
    migrations: %%PHINX_CONFIG_DIR%%/src/sql/migrations

environments:
    default_migration_table: phinxlog
    default_database: development

    development:
        adapter: mysql
        host: @db.unittest.host@
        name: @db.unittest.dbname@
        user: @db.devel.user@
        pass: '@db.devel.passwd@'
        port:
        charset: utf8

