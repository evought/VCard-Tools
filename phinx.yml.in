paths:
    migrations: @vcardtools.migrations.dir@

environments:
    default_migration_table: phinxlog
    default_database: development

    development:
        adapter: mysql
        host: @vcardtools.db.unittest.host@
        name: @vcardtools.db.unittest.dbname@
        user: @vcardtools.db.devel.user@
        pass: '@vcardtools.db.devel.passwd@'
        port:
        charset: utf8

